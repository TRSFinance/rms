<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.trs.rms.mail.bean.MailConfigBean" table="mailconfigbean">
  <id  name="id" column="id" type="java.lang.Long">
   <generator class="native"/>
  </id>
  <property  name="mailEnabled" column="mail_enabled" type="java.lang.Boolean" />
  <property  name="host" column="host" type="java.lang.String"/>
  <property  name="address"  column="address" type="java.lang.String"/>
  <property  name="password" column="password" type="java.lang.String"/>
  <property  name="username" column="username" type="java.lang.String"/>
  <property  name="prefix" column="prefix" type="java.lang.String"/>
  <property  name="port" column="port" type="java.lang.Integer"/>
  <property  name="maxmails" column="maxmails" type="java.lang.Integer"/>
  <property  name="maxsendtimes" column="maxsendtimes" type="java.lang.Integer"/>
 </class>
</hibernate-mapping>
