<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.trs.rms.mail.bean.MailBean" table="mailbean">
   <id  name="id" column="id" type="java.lang.Long">
   <generator class="native"/>
  </id>
  <property name="version" column="version" type="java.lang.Integer"/>
  <property  name="title"  column="title" type="java.lang.String"/>
  <property  name="fileName" column="fileName"  type="java.lang.String"/>
  <property  name="creationDate" column="creationDate"  type="java.sql.Timestamp"/>
  <property  name="sendTimes" column="sendTimes" type="java.lang.Integer" />
  <property  name="module" column="module" type="java.lang.String"/>
  <property  name="status" column="status" type="java.lang.Integer" />
 </class>
</hibernate-mapping>
