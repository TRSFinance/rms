<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.trs.rms.rulemgr.bean.TemplateSyncStat" table="template_sync_stat">
  <id name="id">
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="templateId" type="long" not-null="true"/>
  <property generated="never" lazy="false" name="dasId" type="long" not-null="true"/>
  <property generated="never" lazy="false" name="syncTimes" type="integer"/>
  <property generated="never" lazy="false" name="successTimes" type="integer"/>
  <property generated="never" lazy="false" name="failureTimes" type="integer"/>
  <property generated="never" lazy="false" name="lastSyncTime" type="timestamp" not-null="true"/>
  <property generated="never" lazy="false" name="lastSuccessTime" type="timestamp"/>
  <property generated="never" lazy="false" name="lastFailureTime" type="timestamp"/>
  <!-- <property generated="never" lazy="false" name="lastSuccess" type="boolean"/> -->
 </class>
</hibernate-mapping>
